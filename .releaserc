{
  "branches": [
    "main",
    "eparirishit/1011-consolidate-release-and-release-drafter-workflows"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "writerOpts": {
          "transform": "function (commit, context) {             if (commit.author && typeof commit.author === 'object') {               commit.author = commit.author.name || 'Unknown';             }             return commit;           }"
        }
      }
    ],
    "@semantic-release/changelog",
    "@semantic-release/github"
  ]
}
